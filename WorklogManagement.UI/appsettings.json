{
  "PathBase": "/worklog-management-v2/",
  "ApiBaseAddress": "http://localhost:5510",

  "Serilog": {
    "Using": [
      "Serilog.Expressions",
      "Serilog.Enrichers.UtcTime"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "Enrich": [ "FromLogContext" ],
    "Filter": [
      {
        "Name": "ByIncludingOnly",
        "Args": {
          "expression": "SourceContext like 'WorklogManagement%'"
        }
      }
    ]
  },

  "DetailedErrors": true
}
