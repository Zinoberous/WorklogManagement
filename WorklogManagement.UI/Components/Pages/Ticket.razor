@page "/tickets/{id:int}"
@rendermode InteractiveServer
@inherits BasePage<TicketViewModel>

<PageTitle>Ticket #@Id</PageTitle>

<main class="ticket">
    <AsyncContent IsLoading="@ViewModel.IsLoading.Value" Error="@ViewModel.LoadError.Value?.ToString()">
        Ticket #@Id
    </AsyncContent>
</main>
