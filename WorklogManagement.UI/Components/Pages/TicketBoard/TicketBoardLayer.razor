<FlexCol>
    @foreach (var sub in SubLayer)
    {
        <TicketBoardAccordion @key="sub.Key" Title="@sub.Key" TicketGroups="@sub.Value" OnEdit="@OnEdit" OnDelete="@OnDelete" />
    }
    @if (LayerTickets.Any())
    {
        <TicketBoardDropZone Tickets="@LayerTickets" OnEdit="@OnEdit" OnDelete="@OnDelete" />
    }
</FlexCol>
